source 'https://github.com/CocoaPods/Specs'

platform :ios, '10.0'

use_frameworks!

inhibit_all_warnings!

def all_pods
  pod 'AERecord'
  pod 'SwiftyJSON'
  pod 'SwiftHTTP'
end

target 'GithubJavaDevListBlack' do
  all_pods
end

target 'GithubJavaDevListBlue' do
  all_pods
end

target 'TodayExtForJavaDevListBlack' do
  all_pods
end

target 'TodayExtForJavaDevListBlue' do
  all_pods
end
